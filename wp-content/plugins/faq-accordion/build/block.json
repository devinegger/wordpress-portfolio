{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/faq-accordion",
  "version": "0.1.0",
  "title": "FAQ Accordion",
  "category": "widgets",
  "icon": "format-chat",
  "description": "Individual accordion blocks for adding FAQs to a page in accordion style",
  "attributes": {
    "heading": {
      "selector": "faq-accordion-heading",
      "type": "string"
    },
    "text": {
      "selector": "faq-accordion-text",
      "type": "string"
    },
    "id": {
      "type": "string"
    }
  },
  "supports": {
    "html": false,
    "align": [
      "full",
      "wide"
    ]
  },
  "textdomain": "faq-accordion",
  "viewScript": "file:view.js",
  "editorScript": "file:index.js",
  "editorStyle": "file:index.css",
  "style": "file:style-index.css"
}